<p id="notice"><%= notice %></p>

<h1>Districts</h1>

<table>
  <thead>
    <tr>
      <th>Population</th>
      <th>District type</th>
      <th>Region</th>
      <th>Veteran population</th>
      <th>Median age</th>
      <th>Median income</th>
      <th>Government workers</th>
      <th>Pvi</th>
      <th>State</th>
      <th>Congress</th>
      <th>Politician</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.population %></td>
        <td><%= district.district_type %></td>
        <td><%= district.region %></td>
        <td><%= district.veteran_population %></td>
        <td><%= district.median_age %></td>
        <td><%= district.median_income %></td>
        <td><%= district.government_workers %></td>
        <td><%= district.pvi %></td>
        <td><%= district.state_id %></td>
        <td><%= district.congress_id %></td>
        <td><%= district.politician_id %></td>
        <td><%= link_to 'Show', district %></td>
        <td><%= link_to 'Edit', edit_district_path(district) %></td>
        <td><%= link_to 'Destroy', district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New District', new_district_path %>
